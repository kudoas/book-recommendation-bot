{
  "name": "gas-clasp-starter",
  "version": "1.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest",
    "build": "npm run lint && rimraf dist && webpack && cpx src/appsscript.json dist && cpx \"src/**/*.html\" dist",
    "push": "npm run build && clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howdy39/gas-clasp-starter.git"
  },
  "keywords": [
    "gas",
    "google-apps-script",
    "clasp"
  ],
  "author": "howdy39",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/howdy39/gas-clasp-starter/issues"
  },
  "homepage": "https://github.com/howdy39/gas-clasp-starter#readme",
  "dependencies": {
    "global": "^4.4.0"
  },
  "devDependencies": {
    "@types/google-apps-script": "1.0.57",
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "cpx": "1.5.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "gas-webpack-plugin": "2.3.0",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
