{
  "name": "gas-clasp-starter",
  "version": "1.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest",
    "build": "rimraf dist && webpack && cpx src/appsscript.json dist && cpx \"src/**/*.html\" dist",
    "push": "clasp push"
  },
  "keywords": [
    "gas",
    "google-apps-script",
    "clasp"
  ],
  "license": "MIT",
  "dependencies": {
    "global": "^4.4.0"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "1.0.62",
    "@types/jest": "29.5.1",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "gas-webpack-plugin": "2.4.0",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
